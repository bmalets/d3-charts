<%= element %>_body {
  font: 10px sans-serif;
  min-width: 600px;
  max-height: 400px;
}

<%= element %> { 
  background: #fff;
}

<%= element %> p {
  font: 12px helvetica;
}

<%= element %> .axis path, .axis line {
  fill: none;
  stroke: #000;
  stroke-width: 2px;
  shape-rendering: crispEdges;
}

<%= element %> button {
  position: absolute;
  right: 50px;
  top: 10px;
}
